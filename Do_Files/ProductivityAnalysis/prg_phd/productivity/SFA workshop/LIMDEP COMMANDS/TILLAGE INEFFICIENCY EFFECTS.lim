CALC		;ML=XBR(L)$

CREATE		;LNY=LOG(Y/MY);LNA=LOG(A/MA);LND=LOG(D/MD);LNC=LOG(C/MC);LNL=LOG(L/ML)$

CREATE		;LNAA=LNA*LNA; LNAD=LNA*LND; LNAC=LNA*LNC; LNAL=LNA*LNL; LNDD=LND*LND; LNDC=LND*LNC; LNDL=LND*LNL; LNCC=LNC*LNC
		;LNCL=LNC*LNL; LNLL=LNL*LNL; LNAT=LNA*T; LNDT=LND*T; LNCT=LNC*T; LNLT=LNL*T ;TT=T*T$

NAMELIST	;CD=ONE,LNA,LND,LNC,LNL$
NAMELIST	;TL=LNAA,LNAD,LNAC,LNAL,LNDD,LNDC,LNDL,LNCC,LNCL,LNLL$
NAMELIST	;TCD=T,TT$
NAMELIST	;TTL=T,TT,LNAT,LNDT,LNCT,LNLT$
