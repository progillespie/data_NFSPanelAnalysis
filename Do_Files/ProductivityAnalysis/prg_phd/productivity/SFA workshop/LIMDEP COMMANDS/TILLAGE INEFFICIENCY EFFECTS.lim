? CONVERT CONTINEOUS VARIABLES INTO LOGS (ZEROS STAY AS ZEROS)

CREATE		;LNAGE=LOG(AGE)$
CREATE		;LNALLOC1=LOG(alloc)$
CREATE		;LNFSIZE=LOG(FSIZE)$


? OREA AND KUMBHAKAR (2004)

FRONTIER	;LHS=LNY;RHS=CD,TL,TDV
		;PDS=GRPSIZE
		;MODEL=BC;EFF=UKO
		;HFU=EXTEND,OFFFARM,LNFSIZE,LNAGE,LNALLOC1$

CALC		;TC2005=B(24)$ 
CALC		;TC2006=B(25)$

FRONTIER	;LHS=LNY;RHS=CD,TL,TDV
		;PDS=GRPSIZE
		;MODEL=BC;EFF=UKO;CML:B(24)=TC2005,B(25)=TC2006
		;HFU=EXTEND,OFFFARM,LNFSIZE,LNAGE,LNALLOC1,DECOUP$

CALC ;BEXT   =-0.0183435$
CALC ;BOFFF  =0.0294794$
CALC ;BFSIZE =-0.381041$
CALC ;BAGE   =0.165835$
CALC ;BALLOC1=-0.425852$
CALC ;BDECOUP=0.015187$

CREATE	;EFFSUM=  (EXTEND*BEXT)+(OFFFARM*BOFFF)+(LNFSIZE*BFSIZE)+(LNAGE*BAGE)+(LNALLOC1*BALLOC1)+(DECOUP*BDECOUP)$

CREATE	;UI=UKO/EXP(EFFSUM)$

CREATE	;MUKO=XBR(UKO)$   


?MARGINAL EFFECTS / PARTIAL ELASTICITIES FOR CONTINUOUS VARIABLES?
CREATE			;PEFSIZE=UKO*(BFSIZE)$
CREATE 		;PEAGE=UKO*(BAGE)$
CREATE 		;PEALLOC1=UKO*(BALLOC1)$

?ELASTICITIES FOR CONTINUOUS VARIABLES?
CREATE			;EFSIZE=UKO*(BFSIZE)/MUKO$
CREATE 		;EAGE=UKO*(BAGE)/MUKO$
CREATE 		;EALLOC1=UKO*(BALLOC1)/MUKO$


?DISCRETE AND PERCENTAGE EFFECTS?
CREATE		;ZERO=0$
CREATE		;ONE1=1$


?EXTEND?
CREATE	;EXT_0=UI*EXP((ZERO*BEXT)+(OFFFARM*BOFFF)+(LNFSIZE*BFSIZE)+(LNAGE*BAGE)+(LNALLOC1*BALLOC1)+(DECOUP*BDECOUP))$
  
CREATE	;EXT_1=UI*EXP((ONE1*BEXT)+(OFFFARM*BOFFF)+(LNFSIZE*BFSIZE)+(LNAGE*BAGE)+(LNALLOC1*BALLOC1)+(DECOUP*BDECOUP))$

CREATE	;DEEXT=EXT_1-EXT_0$
CREATE ;PEEXT=DEEXT/UKO$


?OFFFARM?
CREATE	;OFF_0=UI*EXP((EXTEND*BEXT)+(ZERO*BOFFF)+(LNFSIZE*BFSIZE)+(LNAGE*BAGE)+(LNALLOC1*BALLOC1)+(DECOUP*BDECOUP))$
  
CREATE	;OFF_1=UI*EXP((EXTEND*BEXT)+(ONE1*BOFFF)+(LNFSIZE*BFSIZE)+(LNAGE*BAGE)+(LNALLOC1*BALLOC1)+(DECOUP*BDECOUP))$

CREATE	;DEOFF=OFF_1-OFF_0$
CREATE ;PEOFF=DEOFF/UKO$


?DECOUP?
CREATE	;DECOUP_0=UI*EXP((EXTEND*BEXT)+(OFFFARM*BOFFF)+(LNFSIZE*BFSIZE)+(LNAGE*BAGE)+(LNALLOC1*BALLOC1)+(ZERO*BDECOUP))$
  
CREATE	;DECOUP_1=UI*EXP((EXTEND*BEXT)+(OFFFARM*BOFFF)+(LNFSIZE*BFSIZE)+(LNAGE*BAGE)+(LNALLOC1*BALLOC1)+(ONE1*BDECOUP))$

CREATE	;DEDECOUP=DECOUP_1-DECOUP_0$
CREATE ;PEDECOUP=DEDECOUP/UKO$

DSTAT	;Rhs=PEEXT,PEOFF,PEDECOUP,EFSIZE,EAGE,EALLOC1;Wts=w$



